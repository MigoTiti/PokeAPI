<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000"
        motion:staggered="0.5">
        <KeyFrameSet>
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/textView8" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/pokemonSpriteRv" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/textView10" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/pokemonTypeRv" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/textView11" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/pokemonStatRv" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/textView12" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/pokemonAbilityRv" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/textView13" />

            <KeyAttribute
                android:alpha="0"
                motion:framePosition="10"
                motion:motionTarget="@+id/pokemonMoveRv" />

            <KeyPosition
                motion:framePosition="30"
                motion:motionTarget="@+id/pokemonIcon"
                motion:percentX="0.6"
                motion:percentY="0.6" />

            <KeyPosition
                motion:framePosition="30"
                motion:motionTarget="@+id/textView2"
                motion:percentX="0.6"
                motion:percentY="0.6" />

            <KeyPosition
                motion:framePosition="30"
                motion:motionTarget="@+id/textView3"
                motion:percentX="0.6"
                motion:percentY="0.6" />

            <KeyPosition
                motion:framePosition="30"
                motion:motionTarget="@+id/textView4"
                motion:percentX="0.6"
                motion:percentY="0.6" />

            <KeyPosition
                motion:framePosition="30"
                motion:motionTarget="@+id/textView5"
                motion:percentX="0.6"
                motion:percentY="0.6" />

            <KeyPosition
                motion:framePosition="30"
                motion:motionTarget="@+id/textView6"
                motion:percentX="0.6"
                motion:percentY="0.6" />

            <KeyPosition
                motion:framePosition="30"
                motion:motionTarget="@+id/textView7"
                motion:percentX="0.6"
                motion:percentY="0.6" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/textView8"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/pokemonSpriteRv"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/textView10"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/pokemonTypeRv"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/textView11"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/pokemonStatRv"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/textView12"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/pokemonAbilityRv"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/textView13"
                motion:percentY="0.8" />

            <KeyPosition
                motion:framePosition="40"
                motion:motionTarget="@+id/pokemonMoveRv"
                motion:percentY="0.8" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/pokemonIcon"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintDimensionRatio="1:1"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintWidth_percent="0.4">

            <Motion motion:motionStagger="11" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/pokemonIcon">

            <Motion motion:motionStagger="11" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/textView2">

            <Motion motion:motionStagger="11" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/textView2">

            <Motion motion:motionStagger="11" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/barrier">

            <Motion motion:motionStagger="11" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/barrier">

            <Motion motion:motionStagger="11" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/barrier">

            <Motion motion:motionStagger="11" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="10" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/pokemonSpriteRv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="9" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="8" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/pokemonTypeRv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="7" />
            <PropertySet android:alpha="0" />
        </Constraint>
        <Constraint
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="6" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/pokemonStatRv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="5" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="4" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/pokemonAbilityRv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="3" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="2" />
            <PropertySet android:alpha="0" />
        </Constraint>

        <Constraint
            android:id="@+id/pokemonMoveRv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toTopOf="parent"
            motion:layout_constraintStart_toEndOf="parent">

            <Motion motion:motionStagger="1" />
            <PropertySet android:alpha="0" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end" />
</MotionScene>